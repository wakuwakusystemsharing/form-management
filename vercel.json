{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "pnpm build",
  "installCommand": "pnpm install --frozen-lockfile",
  "framework": "nextjs",
  "regions": ["nrt1"],
  "functions": {
    "app/**/*.{js,ts,tsx}": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "git": {
    "deploymentEnabled": {
      "main": true,
      "staging": true
    }
  }
}
